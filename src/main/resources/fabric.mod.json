{
  "schemaVersion": 1,
  "id": "seedlight_riftways",
  "version": "${version}",

  "name": "SeedLight Riftways",
  "description": "With this mod you will be able to seamlessly travel from world to world, from your own front lawn in that single player world you started ages ago to your friend's server, without going through the menus and stuff!",
  "authors": [
    "Emafire003!"
  ],
  "contact": {
    "homepage": "https://github.com/Emafire003/SeedLightRiftways",
    "sources": "https://github.com/Emafire003/SeedLightRiftways",
    "issues": "https://github.com/Emafire003/SeedLightRiftways/issues"
  },

  "license": "GNU LGPL3",
  "icon": "assets/seedlight_riftways/icon.jpg",

  "environment": "*",
  "entrypoints": {
    "main": [
      "me.emafire003.dev.seedlight_riftways.SeedLightRiftways"
    ],
    "client": [
      "me.emafire003.dev.seedlight_riftways.SeedLightRiftwaysClient"
    ],
    "server": [
      "me.emafire003.dev.seedlight_riftways.SeedLightRiftwaysServer"
    ]
  },
  "accessWidener" : "seedlight_riftways.accessWidener",
  "mixins": [
    "seedlight_riftways.mixins.json"
  ],


  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric-api": "*",
    "minecraft": ">=1.19.3",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
