{
  "schemaVersion": 1,
  "id": "seedlight_riftways",
  "version": "${version}",

  "name": "SeedLight Riftways",
  "description": "A mod trying to connect the minecraft multiverse!",
  "authors": [
    "Emafire003!"
  ],
  "contact": {
    "homepage": "https://github.com/Emafire003/SeedLightWorldNetwork",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "ARR",
  "icon": "assets/seedlight_riftways/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "me.emafire003.dev.seedlight_riftways.SeedLightRiftways"
    ],
    "client": [
      "me.emafire003.dev.seedlight_riftways.client.SeedLightRiftwaysClient"
    ]
  },
  "accessWidener" : "seedlight_riftways.accessWidener",
  "mixins": [
    "seedlight_riftways.mixins.json"
